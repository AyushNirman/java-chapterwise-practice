class Main {
    public static void main(String[] args) {

        String[] arr = {"Java", "Python", "C++"};
        String key = "Python";

        int index = -1;

        for (int i = 0; i < arr.length; i++) {
            if (arr[i].equals(key)) {
                index = i;
                break;
            }
        }

        System.out.println(index);
    }
}

public class Main {
    public static void main(String[] args) {

        int[] arr1 = {1, 2, 3, 4, 5};
        int[] arr2 = new int[arr1.length];

        for (int i = 0; i < arr1.length; i++) {
            arr2[i] = arr1[i];
        }

        // Print copied array
        for (int num : arr2) {
            System.out.print(num + " ");
        }
    }
}
////////////////////////
int[] arr1 = {1, 2, 3, 4};
int[] arr2 = arr1.clone();

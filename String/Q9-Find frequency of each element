import java.util.*; 
// Imports all utility classes like Map and HashMap

public class Main {
    public static void main(String[] args) {

        String s = "java"; 
        // Input string whose character frequency we want to count

        Map<Character, Integer> map = new HashMap<>();
        // HashMap to store character as key and its frequency as value

        for (char c : s.toCharArray()) {
            // Convert string into character array and traverse each character

            map.put(c, map.getOrDefault(c, 0) + 1);
            // If character exists, increase its count by 1
            // If character does not exist, start count from 1
        }

        System.out.println(map);
        // Print final character frequency map
    }
}

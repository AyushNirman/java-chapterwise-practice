public class Main {
    public static void main(String[] args) {
        // Step 1: start values (first two Fibonacci numbers)
        int a = 0;      // F0
        int b = 1;      // F1

        // Step 2: how many terms you want
        int n = 10;     // print first 10 terms

        // Step 3: print the first two terms
        System.out.print(a + " " + b);

        // Step 4: loop from the 3rd term (i = 2) up to n-1
        for (int i = 2; i < n; i++) {
            // Step 5: compute next term as sum of previous two
            int c = a + b;      // c = next Fibonacci number

            // Step 6: print the new term
            System.out.print(" " + c);

            // Step 7: shift the last two values forward
            a = b;              // old b becomes new a
            b = c;              // new c becomes new b
        }
    }
}
